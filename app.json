{
  "expo": {
    "name": "SimpFi",
    "slug": "SimpFi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo.jpg",
    "scheme": "com.simpleLife.simpFi",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.simpleLife.simpFi",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Logo.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.simpleLife.simpFi"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Logo.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-asset",
        {
          "assets": [
            "./assets/classifier/category_classifier.json"
          ]
        }
      ],
      "expo-font",
      [
        "@react-native-google-signin/google-signin"
      ],
      [
        "react-native-document-scanner-plugin",
        {
          "cameraPermission": "We need camera access, so you can scan receipts."
        }
      ],
      "expo-build-properties",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1e9cfe97-1523-44b3-8a59-f1d8e50023a3"
      }
    }
  }
}
