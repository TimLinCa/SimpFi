{
  "expo": {
    "name": "billmate",
    "slug": "billmate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo.jpg",
    "scheme": "com.simplelife.billmate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Logo.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.timlin626.billmate"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Logo.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-asset",
        {
          "assets": [
            "./assets/classifier/category_classifier.json"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "288a4703-262f-4462-ab63-158122c370e2"
      }
    },
    "owner": "timlin626"
  }
}